    "react-dom": "^19.2.0",
    "react-i18next": "^16.4.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "vite-plugin-pwa": "^1.2.0",
    "xlsx": "^0.18.5"
